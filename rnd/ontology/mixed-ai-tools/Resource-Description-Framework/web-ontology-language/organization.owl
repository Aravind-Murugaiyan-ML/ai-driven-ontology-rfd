<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/ontology#"
     xml:base="http://example.org/ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/ontology#Organization"/>
    <owl:Class rdf:about="http://example.org/ontology#Department"/>
    <owl:Class rdf:about="http://example.org/ontology#Goal"/>
    <owl:Class rdf:about="http://example.org/ontology#KPI"/>
    <owl:Class rdf:about="http://example.org/ontology#Budget"/>
    <owl:Class rdf:about="http://example.org/ontology#ImplementationPlan"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasDepartment">
        <rdfs:domain rdf:resource="http://example.org/ontology#Organization"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Department"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasGoal">
        <rdfs:domain rdf:resource="http://example.org/ontology#Department"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Goal"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="http://example.org/ontology#trackedBy">
        <rdfs:domain rdf:resource="http://example.org/ontology#Goal"/>
        <rdfs:range rdf:resource="http://example.org/ontology#KPI"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasBudget">
        <rdfs:domain rdf:resource="http://example.org/ontology#Goal"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Budget"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="http://example.org/ontology#implementedBy">
        <rdfs:domain rdf:resource="http://example.org/ontology#Goal"/>
        <rdfs:range rdf:resource="http://example.org/ontology#ImplementationPlan"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#name">
        <rdfs:domain rdf:resource="http://example.org/ontology#Organization"/>
        <rdfs:domain rdf:resource="http://example.org/ontology#Department"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#description">
        <rdfs:domain rdf:resource="http://example.org/ontology#Goal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#metric">
        <rdfs:domain rdf:resource="http://example.org/ontology#KPI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#amount">
        <rdfs:domain rdf:resource="http://example.org/ontology#Budget"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#planDetails">
        <rdfs:domain rdf:resource="http://example.org/ontology#ImplementationPlan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

</rdf:RDF>
