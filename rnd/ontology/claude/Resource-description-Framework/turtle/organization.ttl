@prefix : <http://example.org/strategy-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

<http://example.org/strategy-ontology> rdf:type owl:Ontology ;
    dcterms:title "Organizational Strategy Ontology" ;
    dcterms:description "An ontology for organizational strategy, vision, and planning capturing entities like departments, goals, KPIs, budgets, and their relationships across multiple organizational levels." ;
    owl:versionInfo "1.0" .

#################################################################
#    Classes
#################################################################

### Core Organizational Entities

:Organization rdf:type owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "A business organization or company entity" .

:Department rdf:type owl:Class ;
    rdfs:label "Department" ;
    rdfs:comment "An organizational unit within a company" .

:Stakeholder rdf:type owl:Class ;
    rdfs:label "Stakeholder" ;
    rdfs:comment "Individual or entity with interest in organizational outcomes" .


### Strategic Planning Entities

:Strategy rdf:type owl:Class ;
    rdfs:label "Strategy" ;
    rdfs:comment "High-level organizational strategy" .

:StrategicGoal rdf:type owl:Class ;
    rdfs:label "Strategic Goal" ;
    rdfs:comment "Organization-wide strategic objective" .

:DepartmentPlan rdf:type owl:Class ;
    rdfs:label "Department Plan" ;
    rdfs:comment "Department-level planning document aligned with organizational strategy" .

:DepartmentGoal rdf:type owl:Class ;
    rdfs:label "Department Goal" ;
    rdfs:comment "Department-specific goal supporting strategic objectives" .



### Implementation Entities

:Initiative rdf:type owl:Class ;
    rdfs:label "Initiative" ;
    rdfs:comment "Specific project or program to achieve goals" .

:Milestone rdf:type owl:Class ;
    rdfs:label "Milestone" ;
    rdfs:comment "Key checkpoint or deliverable within an initiative" .

### Measurement Entities

:KPI rdf:type owl:Class ;
    rdfs:label "Key Performance Indicator" ;
    rdfs:comment "Measurable value demonstrating goal achievement" .

:MetricData rdf:type owl:Class ;
    rdfs:label "Metric Data" ;
    rdfs:comment "Actual measurement data for KPIs" .

:GoalKPI rdf:type owl:Class ;
    rdfs:label "Goal KPI Measurement" ;
    rdfs:comment "Association between goals and their KPI measurements" .

### Financial Entities

:Budget rdf:type owl:Class ;
    rdfs:label "Budget" ;
    rdfs:comment "Financial allocation for department operations" .

:BudgetAllocation rdf:type owl:Class ;
    rdfs:label "Budget Allocation" ;
    rdfs:comment "Specific financial allocation from budget to plan" .

### Association Entities

:StrategyStakeholder rdf:type owl:Class ;
    rdfs:label "Strategy Stakeholder Association" ;
    rdfs:comment "Association between strategy and stakeholder involvement" .

#################################################################
#    Object Properties
#################################################################

### Organizational Structure

:belongsTo rdf:type owl:ObjectProperty ;
    rdfs:label "belongs to" ;
    rdfs:comment "Indicates organizational membership" .

:contains rdf:type owl:ObjectProperty ;
    rdfs:label "contains" ;
    rdfs:comment "Indicates containment relationship" ;
    owl:inverseOf :belongsTo .

:manages rdf:type owl:ObjectProperty ;
    rdfs:label "manages" ;
    rdfs:comment "Indicates management responsibility" .

### Strategic Relationships

:defines rdf:type owl:ObjectProperty ;
    rdfs:label "defines" ;
    rdfs:comment "Organization defines strategy" ;
    rdfs:domain :Organization ;
    rdfs:range :Strategy .

:includes rdf:type owl:ObjectProperty ;
    rdfs:label "includes" ;
    rdfs:comment "Strategy includes strategic goals" ;
    rdfs:domain :Strategy ;
    rdfs:range :StrategicGoal .

:guides rdf:type owl:ObjectProperty ;
    rdfs:label "guides" ;
    rdfs:comment "Strategy guides department plans" ;
    rdfs:domain :Strategy ;
    rdfs:range :DepartmentPlan .

:creates rdf:type owl:ObjectProperty ;
    rdfs:label "creates" ;
    rdfs:comment "Department creates plans" ;
    rdfs:domain :Department ;
    rdfs:range :DepartmentPlan .

:cascadesTo rdf:type owl:ObjectProperty ;
    rdfs:label "cascades to" ;
    rdfs:comment "Strategic goal cascades to department goal" ;
    rdfs:domain :StrategicGoal ;
    rdfs:range :DepartmentGoal .

### Implementation Relationships

:implementsThrough rdf:type owl:ObjectProperty ;
    rdfs:label "implements through" ;
    rdfs:comment "Plan implements through initiatives" ;
    rdfs:domain :DepartmentPlan ;
    rdfs:range :Initiative .

:achievedThrough rdf:type owl:ObjectProperty ;
    rdfs:label "achieved through" ;
    rdfs:comment "Goal achieved through initiatives" ;
    rdfs:domain :DepartmentGoal ;
    rdfs:range :Initiative .

:brokenInto rdf:type owl:ObjectProperty ;
    rdfs:label "broken into" ;
    rdfs:comment "Initiative broken into milestones" ;
    rdfs:domain :Initiative ;
    rdfs:range :Milestone .

### Measurement Relationships

:measuredBy rdf:type owl:ObjectProperty ;
    rdfs:label "measured by" ;
    rdfs:comment "Goal measured by KPI association" ;
    rdfs:range :GoalKPI .

:trackedBy rdf:type owl:ObjectProperty ;
    rdfs:label "tracked by" ;
    rdfs:comment "Department goal tracked by KPI" ;
    rdfs:domain :DepartmentGoal ;
    rdfs:range :GoalKPI .

:usedIn rdf:type owl:ObjectProperty ;
    rdfs:label "used in" ;
    rdfs:comment "KPI used in goal measurement" ;
    rdfs:domain :KPI ;
    rdfs:range :GoalKPI .

:generates rdf:type owl:ObjectProperty ;
    rdfs:label "generates" ;
    rdfs:comment "KPI generates metric data" ;
    rdfs:domain :KPI ;
    rdfs:range :MetricData .

### Financial Relationships

:fundedBy rdf:type owl:ObjectProperty ;
    rdfs:label "funded by" ;
    rdfs:comment "Plan funded by budget allocation" ;
    rdfs:domain :DepartmentPlan ;
    rdfs:range :BudgetAllocation .

:distributedAs rdf:type owl:ObjectProperty ;
    rdfs:label "distributed as" ;
    rdfs:comment "Budget distributed as allocations" ;
    rdfs:domain :Budget ;
    rdfs:range :BudgetAllocation .

### Stakeholder Relationships

:involves rdf:type owl:ObjectProperty ;
    rdfs:label "involves" ;
    rdfs:comment "Strategy involves stakeholders" ;
    rdfs:domain :Strategy ;
    rdfs:range :StrategyStakeholder .

:participatesIn rdf:type owl:ObjectProperty ;
    rdfs:label "participates in" ;
    rdfs:comment "Stakeholder participates in strategy" ;
    rdfs:domain :Stakeholder ;
    rdfs:range :StrategyStakeholder .

#################################################################
#    Data Properties
#################################################################

### Identifiers

:orgId rdf:type owl:DatatypeProperty ;
    rdfs:label "organization ID" ;
    rdfs:domain :Organization ;
    rdfs:range xsd:string .

:strategyId rdf:type owl:DatatypeProperty ;
    rdfs:label "strategy ID" ;
    rdfs:domain :Strategy ;
    rdfs:range xsd:string .

:deptId rdf:type owl:DatatypeProperty ;
    rdfs:label "department ID" ;
    rdfs:domain :Department ;
    rdfs:range xsd:string .

:goalId rdf:type owl:DatatypeProperty ;
    rdfs:label "goal ID" ;
    rdfs:range xsd:string .

:kpiId rdf:type owl:DatatypeProperty ;
    rdfs:label "KPI ID" ;
    rdfs:domain :KPI ;
    rdfs:range xsd:string .

### Descriptive Properties

:name rdf:type owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:range xsd:string .

:description rdf:type owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:range xsd:string .

:title rdf:type owl:DatatypeProperty ;
    rdfs:label "title" ;
    rdfs:range xsd:string .

:mission rdf:type owl:DatatypeProperty ;
    rdfs:label "mission" ;
    rdfs:domain :Organization ;
    rdfs:range xsd:string .

:vision rdf:type owl:DatatypeProperty ;
    rdfs:label "vision" ;
    rdfs:domain :Organization ;
    rdfs:range xsd:string .

:industry rdf:type owl:DatatypeProperty ;
    rdfs:label "industry" ;
    rdfs:domain :Organization ;
    rdfs:range xsd:string .

### Temporal Properties

:startDate rdf:type owl:DatatypeProperty ;
    rdfs:label "start date" ;
    rdfs:range xsd:date .

:endDate rdf:type owl:DatatypeProperty ;
    rdfs:label "end date" ;
    rdfs:range xsd:date .

:targetDate rdf:type owl:DatatypeProperty ;
    rdfs:label "target date" ;
    rdfs:range xsd:date .

:actualDate rdf:type owl:DatatypeProperty ;
    rdfs:label "actual date" ;
    rdfs:range xsd:date .

:established rdf:type owl:DatatypeProperty ;
    rdfs:label "established date" ;
    rdfs:domain :Organization ;
    rdfs:range xsd:date .

:measurementDate rdf:type owl:DatatypeProperty ;
    rdfs:label "measurement date" ;
    rdfs:domain :MetricData ;
    rdfs:range xsd:date .

:createdDate rdf:type owl:DatatypeProperty ;
    rdfs:label "created date" ;
    rdfs:range xsd:date .

### Status Properties

:status rdf:type owl:DatatypeProperty ;
    rdfs:label "status" ;
    rdfs:range xsd:string .

:priorityLevel rdf:type owl:DatatypeProperty ;
    rdfs:label "priority level" ;
    rdfs:range xsd:integer .

### Measurement Properties

:measurementUnit rdf:type owl:DatatypeProperty ;
    rdfs:label "measurement unit" ;
    rdfs:domain :KPI ;
    rdfs:range xsd:string .

:calculationMethod rdf:type owl:DatatypeProperty ;
    rdfs:label "calculation method" ;
    rdfs:domain :KPI ;
    rdfs:range xsd:string .

:frequency rdf:type owl:DatatypeProperty ;
    rdfs:label "frequency" ;
    rdfs:domain :KPI ;
    rdfs:range xsd:string .

:targetValue rdf:type owl:DatatypeProperty ;
    rdfs:label "target value" ;
    rdfs:domain :GoalKPI ;
    rdfs:range xsd:decimal .

:currentValue rdf:type owl:DatatypeProperty ;
    rdfs:label "current value" ;
    rdfs:domain :GoalKPI ;
    rdfs:range xsd:decimal .

:value rdf:type owl:DatatypeProperty ;
    rdfs:label "value" ;
    rdfs:domain :MetricData ;
    rdfs:range xsd:decimal .

:measurementPeriod rdf:type owl:DatatypeProperty ;
    rdfs:label "measurement period" ;
    rdfs:range xsd:string .

:period rdf:type owl:DatatypeProperty ;
    rdfs:label "period" ;
    rdfs:domain :MetricData ;
    rdfs:range xsd:string .

:dataSource rdf:type owl:DatatypeProperty ;
    rdfs:label "data source" ;
    rdfs:domain :MetricData ;
    rdfs:range xsd:string .

### Financial Properties

:totalAmount rdf:type owl:DatatypeProperty ;
    rdfs:label "total amount" ;
    rdfs:domain :Budget ;
    rdfs:range xsd:decimal .

:allocatedAmount rdf:type owl:DatatypeProperty ;
    rdfs:label "allocated amount" ;
    rdfs:domain :BudgetAllocation ;
    rdfs:range xsd:decimal .

:budgetRequired rdf:type owl:DatatypeProperty ;
    rdfs:label "budget required" ;
    rdfs:domain :Initiative ;
    rdfs:range xsd:decimal .

:currency rdf:type owl:DatatypeProperty ;
    rdfs:label "currency" ;
    rdfs:domain :Budget ;
    rdfs:range xsd:string .

:fiscalYear rdf:type owl:DatatypeProperty ;
    rdfs:label "fiscal year" ;
    rdfs:domain :Budget ;
    rdfs:range xsd:integer .

:category rdf:type owl:DatatypeProperty ;
    rdfs:label "category" ;
    rdfs:domain :BudgetAllocation ;
    rdfs:range xsd:string .

### Organizational Properties

:headName rdf:type owl:DatatypeProperty ;
    rdfs:label "head name" ;
    rdfs:domain :Department ;
    rdfs:range xsd:string .

:employeeCount rdf:type owl:DatatypeProperty ;
    rdfs:label "employee count" ;
    rdfs:domain :Department ;
    rdfs:range xsd:integer .

:owner rdf:type owl:DatatypeProperty ;
    rdfs:label "owner" ;
    rdfs:domain :Initiative ;
    rdfs:range xsd:string .

### Stakeholder Properties

:role rdf:type owl:DatatypeProperty ;
    rdfs:label "role" ;
    rdfs:domain :Stakeholder ;
    rdfs:range xsd:string .

:contactInfo rdf:type owl:DatatypeProperty ;
    rdfs:label "contact info" ;
    rdfs:domain :Stakeholder ;
    rdfs:range xsd:string .

:influenceLevel rdf:type owl:DatatypeProperty ;
    rdfs:label "influence level" ;
    rdfs:domain :Stakeholder ;
    rdfs:range xsd:string .

:involvementType rdf:type owl:DatatypeProperty ;
    rdfs:label "involvement type" ;
    rdfs:domain :StrategyStakeholder ;
    rdfs:range xsd:string .

:responsibility rdf:type owl:DatatypeProperty ;
    rdfs:label "responsibility" ;
    rdfs:domain :StrategyStakeholder ;
    rdfs:range xsd:string .

